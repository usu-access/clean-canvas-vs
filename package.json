{
  "name": "clean-canvas",
  "displayName": "clean-canvas",
  "description": "Converts HTML formatted using Design tools in Canvas to HTML ready for upload to the Omni CMS.",
  "version": "1.0.0",
  "icon": "images/Clean-Canvas.png",
  "publisher": "clean-canvas",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:clean-canvas.cleanCanvas"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "clean-canvas.cleanCanvas",
      "title": "start clean-canvas"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
